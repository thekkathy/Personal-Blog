   });\r\n    } else {\r\n      fetch(\"http://localhost:8000/blog_posts/add\", {\r\n        method: \"POST\",\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n        },\r\n        body: JSON.stringify({\r\n          title: title,\r\n          text: text,\r\n          pic_url: pic,\r\n        }),\r\n      }).then((resp) => {\r\n        resp.json();\r\n        history.push(\"/blog\");\r\n      });\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div class=\"card\">\r\n      <form onSubmit={submitForm}>\r\n        <div class=\"form-group\">\r\n          <input\r\n            defaultValue={title}\r\n            type=\"text\"\r\n            class=\"form-control item\"\r\n            id=\"username\"\r\n            placeholder=\"Title\"\r\n            onChange={(e) => setTitle(e.target.value)}\r\n          />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <textarea\r\n            defaultValue={text}\r\n            type=\"text\"\r\n            class=\"form-control item\"\r\n            id=\"email\"\r\n            placeholder=\"Your blog post\"\r\n            onChange={(e) => setText(e.target.value)}\r\n          />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <input\r\n            defaultValue={pic}\r\n            type=\"text\"\r\n            class=\"form-control item\"\r\n            id=\"phone-number\"\r\n            placeholder=\"Picture URL\"\r\n            onChange={(e) => setPic(e.target.value)}\r\n          />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <button type=\"submit\" class=\"btn btn-primary\">\r\n            {isEdit === true ? \"Edit Post\" : \"Create Post\"}\r\n          </button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  );\r\n}\r\nexport default PostInput;\r\n","C:\\Users\\Taher Calcuttawala\\Documents\\GitHub\\Personal-Blog\\client\\src\\components\\Blog\\BlogCard.js",[],"C:\\Users\\Taher Calcuttawala\\Documents\\GitHub\\Personal-Blog\\client\\src