t) => {\r\n          return (\r\n            <div class=\"col\">\r\n              <BlogCard post={post} auth={true} />\r\n            </div>\r\n          );\r\n        })}\r\n        {isAuth ? <NewPostCard></NewPostCard> : null}\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Blog;\r\n","C:\\Users\\Taher Calcuttawala\\Documents\\GitHub\\Personal-Blog\\client\\src\\components\\PostContentFormat.js",[],"C:\\Users\\Taher Calcuttawala\\Documents\\GitHub\\Personal-Blog\\client\\src\\components\\Comments\\Comments.js",[],"C:\\Users\\Taher Calcuttawala\\Documents\\GitHub\\Personal-Blog\\client\\src\\components\\Shop\\ShopCard.js",["140"],"import React from 'react'\r\nimport Card from '../Card';\r\n\r\nconst ShopCard = () => {\r\n    return (\r\n        <div>\r\n            ShopCard - uses the card design\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default ShopCard\r\n","C:\\Users\\Taher Calcuttawala\\Documents\\GitHub\\Personal-Blog\\client\\src\\utils\\getBlogPosts.js",[],"C:\\Users\\Taher Calcuttawala\\Documents\\GitHub\\Personal-Blog\\client\\src\\components\\Blog\\PostInput.js",["141","142"],"C:\\Users\\Taher Calcuttawala\\Documents\\GitHub\\Personal-Blog\\client\\src\\components\\Blog\\BlogCard.js",[],"C:\\Users\\Taher Calcuttawala\\Documents\\GitHub\\Personal-Blog\\client\\src\\components\\Blog\\NewPostCard.js",[],"C:\\Users\\Taher Calcuttawala\\Documents\\GitHub\\Personal-Blog\\client\\src\\components\\auth\\SignIn.js",["143","144"],"import {useState,useEffect,useContext } from \"react\";\r\nimport { Button } from 'react-bootstrap';\r\nimport { signInWithGoogle,auth } from \"../../firebase\";\r\nimport {UsersContext} from '../../context/usersContext'\r\n\r\nexport default function SignIn(){\r\n\r\n  const { users, setUsers } = useContext(UsersContext);\r\n\r\n  useEffect(()=>{\r\n    console.log(users);\r\n  },[users])\r\n\r\n  function handleSignIn(){\r\n    signInWithGoogle(users,setUsers);\r\n  }\r\n\r\n  return (\r\n    <div>\r\n        <Butt